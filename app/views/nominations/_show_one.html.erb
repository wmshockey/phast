<h2>Nomination</h2>
<table class="fixed", style="width: 1200px;">
  <thead>
    <tr>
      <th>Nomination Name</th>
	  <th>Description</th>
	  <th>Nomination Date</th>
	  <th>Period (days)</th>
	  <th>Pipeline</th>
    </tr>
  </thead>

  <tbody>
    <tr>
      <td><%= @nomination.name %></td>
      <td><%= @nomination.description %></td>
	  <td><%= @nomination.nom_date %></td>
	  <td><%= @nomination.period %></td>
	  <td><%= @nomination.pipeline_name %></td>
    </tr>
  </tbody>
</table>